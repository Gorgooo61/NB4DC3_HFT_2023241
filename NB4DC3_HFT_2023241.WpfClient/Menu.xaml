<Window x:Class="NB4DC3_HFT_2023241.WpfClient.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NB4DC3_HFT_2023241.WpfClient"
        mc:Ignorable="d"
        Title="Menu" Height="450" Width="800">
    <Grid Background="LightBlue">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Button x:Name="BrandButton" Content="Brand" HorizontalAlignment="Center" VerticalAlignment="Center" Height="70" Width="100" Background="WhiteSmoke" Grid.Row="0" Click="BrandButton_ClickB"/>
        <Button x:Name="CarButton" Content="Car" HorizontalAlignment="Center" VerticalAlignment="Center" Height="70" Width="100" Background="WhiteSmoke" Grid.Row="1" Click="CarButton_ClickC"/>
        <Button x:Name="OrderButton" Content="Order" HorizontalAlignment="Center" VerticalAlignment="Center" Height="70" Width="100" Background="WhiteSmoke" Grid.Row="2" Click="OrderButton_ClickO"/>
    </Grid>
</Window>
